{
  "version": 2,
  "builds": [
    { "src": "Client/public/**/*", "use": "@vercel/static" },
    { "src": "server/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/server/$1" },
    { "src": "/(.*)", "dest": "/Client/public/$1" }
  ]
}
